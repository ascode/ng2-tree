{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ng2-tree"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist-demo",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "demo/index.html",
      "main": "demo/main.ts",
      "polyfills": "demo/polyfills.ts",
      "test": "../test/test.ts",
      "tsconfig": "demo/tsconfig.app.json",
      "testTsconfig": "../test/tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/font-awesome/css/font-awesome.min.css"
      ],
      "scripts": [],
      "environmentSource": "demo/environments/environment.ts",
      "environments": {
        "dev": "demo/environments/environment.ts",
        "prod": "demo/environments/environment.prod.ts"
      }
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/demo/tsconfig.app.json"
    },
    {
      "project": "test/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
